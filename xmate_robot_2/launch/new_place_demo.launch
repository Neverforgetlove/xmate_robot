<?xml version='1.0' encoding='utf-8'?>

<launch>
  <node pkg="xmate_robot_2" type="new_place_demo" name="xmate_robot" output="screen" >
    
    <!--设置机械臂运行速度-->
    <param name="move_sped" value="0.3"/>

    <!--螺丝放置位置对应Ar码号-->
    <param name="LS_ID" value="6"/>
    <!--螺母放置位置对应Ar码号-->
    <param name="LM_ID" value="8"/>
    <!--钣金件放置位置对应Ar码号-->
    <param name="BJJ_ID" value="2"/>

  <!-- 物料id1从物料盘上面抓取参数 -->
    <!-- 从物料盘抓取id1物料时机械臂的x坐标偏移量(笛卡尔坐标系)，减小向前(默认：0.015) -->
    <param name="placeid1_x" value="-0.005"/>　<!-- 单位：m-->
    <!-- 从物料盘抓取id1物料时机械臂的y坐标偏移量(笛卡尔坐标系)，增大向左(默认：0.004) -->
    <param name="placeid1_y" value="-0.22"/>　<!-- 单位：m-->
    <!-- 从物料盘抓取id1物料时机械臂的z坐标偏移量(笛卡尔坐标系)，减小往上(默认：0.040134663) -->
    <param name="placeid1_z" value="0.063"/>　<!-- 单位：m-->

    <!-- 物料id2螺母从物料盘上面抓取参数 -->
    <!-- 从物料盘抓取id2物料时机械臂的x坐标偏移量(笛卡尔坐标系)，减小向前(默认：0.0) -->
    <param name="placeid2_x" value="-0.005"/>　<!-- 单位：m-->
    <!-- 从物料盘抓取id2物料时机械臂的y坐标偏移量(笛卡尔坐标系)，增大向左(默认：0.1115) -->
    <param name="placeid2_y" value="0.0035"/>　<!-- 单位：m-->
    <!-- 从物料盘抓取id2物料时机械臂的z坐标偏移量(笛卡尔坐标系)，减小往上(默认：0.041234663) -->
    <param name="placeid2_z" value="0.063"/>　<!-- 单位：m-->

    <!-- 钣金件从物料盘上面抓取参数 -->
    <!-- 从物料盘抓取id2物料时机械臂的x坐标偏移量(笛卡尔坐标系)，增大往后(默认：0.0) -->
    <param name="zpt_bjj_grasp_x" value="0.0430802"/>　<!-- 单位：m-->
    <!-- 从物料盘抓取id2物料时机械臂的y坐标偏移量(笛卡尔坐标系)，增大向左(默认：0.1115) -->
    <param name="zpt_bjj_grasp_y" value="-0.109"/>　<!-- 单位：m-->
    <!-- 从物料盘抓取id2物料时机械臂的z坐标偏移量(笛卡尔坐标系)，增大往下(默认：0.041234663) -->
    <param name="zpt_bjj_grasp_z" value="0.0420"/>　<!-- 单位：m-->
    
     <!-- 钣金件放到装配台上面放置参数 -->
    <!-- 从物料盘抓取id2物料时机械臂的x坐标偏移量(笛卡尔坐标系)，增大往左(默认：0.0) -->
    <param name="zpt_bjj_place_x" value="0.019"/>　<!-- 单位：m-->
    <!-- 从物料盘抓取id2物料时机械臂的y坐标偏移量(笛卡尔坐标系)，增大向前(默认：0.1115) -->
    <param name="zpt_bjj_place_y" value="0.056"/>　<!-- 单位：m 0.0945-->
    <!-- 从物料盘抓取id2物料时机械臂的z坐标偏移量(笛卡尔坐标系)，增大往上(默认：0.041234663) -->
    <param name="zpt_bjj_place_z" value="0.216"/>　<!-- 单位：m-->


    <!-- 设置放置物料参数 -->
    <!-- 放置物料到装配台时机械臂的x坐标偏移量(笛卡尔坐标系)，增大往左(默认：0.020) -->
    <param name="place_x" value="0.018"/>　<!-- 单位：m-->
    <!-- 放置物料到装配台时机械臂的y坐标偏移量(笛卡尔坐标系)，增大向前(默认：0.032) -->
    <param name="place_y" value="0.0445"/>　<!-- 单位：m-->
    <!-- 放置物料到装配台时机械臂的z坐标偏移量(笛卡尔坐标系)，增大往上(默认：0.228) -->  
    <param name="place_z" value="0.217"/>　<!-- 单位：m-->

  </node>

</launch>
